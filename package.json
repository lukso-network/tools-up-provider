{
  "name": "tools-up-provider",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev": "turbo dev --no-cache --continue",
    "lint": "biome check .",
    "lint:fix": "biome check . --apply",
    "clean": "turbo clean && rm -rf node_modules temp",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "changeset": "changeset",
    "release": "turbo build --filter=docs^... && changeset publish",
    "test": "turbo test",
    "version-packages": "turbo run build lint test",
    "publish-packages": "turbo run build lint test && changeset version && changeset publish",
    "wrangler:deploy": "turbo run wrangler:deploy",
    "wrangler:preview": "turbo run wrangler:preview",
    "gendocs": "turbo run docs && typedoc --githubPages --entryPointStrategy merge ./.docs-json/*.json -out docs"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.8.1",
    "@types/node-fetch": "^2.6.11",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.5",
    "jest": "^29.7.0",
    "source-map-support": "^0.5.21",
    "tsup": "^8.3.5",
    "turbo": "^2.2.3",
    "zx": "^8.1.9"
  },
  "packageManager": "pnpm@9.5.0",
  "workspaces": [
    "packages/*",
    "configs/*",
    "examples/astro-example"
  ],
  "dependencies": {
    "typedoc": "^0.26.10",
    "wrangler": "^3.83.0"
  }
}
