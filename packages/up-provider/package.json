{
  "name": "@lukso/up-provider",
  "version": "0.3.5-test8",
  "packageManager": "yarn@3.3.0",
  "publishConfig": {
    "@lukso:registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lukso-network/tools-up-provider.git"
  },
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@mixer/postmessage-rpc": "^1.1.4",
    "@types/debug": "^4.1.12",
    "@types/uuid": "^10.0.0",
    "debug": "^4.4.0",
    "eventemitter3": "^5.0.1",
    "json-rpc-2.0": "^1.7.0",
    "lit": "^3.2.1",
    "pauseable": "^0.3.2",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "scripts": {
    "build": "tsup ./src",
    "dev": "tsup ./src --watch",
    "docs": "path=$(pwd) && name=$(basename $path) && mkdir -p ../../.docs-json && (cd ../.. && ls ./packages/$name && typedoc --json .docs-json/$name.json --tsconfig ./packages/$name/tsconfig.json --options ./packages/$name/typedoc.json)"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "default": "./dist/index.global.js"
    },
    "./popup": {
      "types": "./dist/popup.d.ts",
      "require": "./dist/popup.js",
      "import": "./dist/popup.mjs",
      "default": "./dist/popup.global.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "require": "./dist/server.js",
      "import": "./dist/server.mjs",
      "default": "./dist/server.global.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "require": "./dist/client.js",
      "import": "./dist/client.mjs",
      "default": "./dist/client.global.js"
    },
    "./src/*": "./src/*"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "unpkg": "./dist/index.global.js",
  "jsdelivr": "./dist/index.global.js"
}
