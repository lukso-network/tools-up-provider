{
  "name": "@lukso/up-provider",
  "packageManager": "yarn@3.3.0",
  "files": [
    "dist",
    "src"
  ],
  "dependencies": {
    "@mixer/postmessage-rpc": "^1.1.4",
    "@types/debug": "^4.1.12",
    "@types/uuid": "^10.0.0",
    "debug": "^4.3.7",
    "eventemitter3": "^5.0.1",
    "json-rpc-2.0": "^1.7.0",
    "lit": "^3.2.1",
    "pauseable": "^0.3.2",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "tsup": "^8.3.0",
    "typescript": "^5.6.2"
  },
  "scripts": {
    "build": "tsup ./src",
    "dev": "tsup ./src --watch",
    "docs": "path=$(pwd) && name=$(basename $path) && mkdir -p ../../.docs-json && (cd ../.. && ls ./packages/$name && typedoc --json .docs-json/$name.json --tsconfig ./packages/$name/tsconfig.json --options ./packages/$name/typedoc.json)"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./src/*": "./src/*"
  },
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "version": "0.1.3"
}
